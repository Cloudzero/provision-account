{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "aws-portal:*PaymentMethods",
                "aws-portal:*Account",
                "ec2:GetPasswordData",
                "ec2:GetConsoleOutput",
                "ec2:GetConsoleScreenshot",
                "glue:GetConnection"
            ],
            "Resource": "*",
            "Effect": "Deny",
            "Sid": "CZProtect20190912"
        },
        {
            "Action": [
                "aws-portal:View*",
                "budgets:*",
                "pricing:*"
            ],
            "Resource": "*",
            "Effect": "Allow",
            "Sid": "CZBudgetMonitoring20190912"
        },
        {
            "Action": [
                "xray:Get*",
                "xray:Batch*",
                "cloudtrail:Get*",
                "cloudtrail:List*",
                "cloudtrail:Describe*",
                "health:*"
            ],
            "Resource": "*",
            "Effect": "Allow",
            "Sid": "CZActivityMonitoring20190912"
        },
        {
            "Action": [
                "account:List*",
                "amplify:List*",
                "apigateway:GET",
                "apigateway:HEAD",
                "apigateway:OPTIONS",
                "applicationinsights:Describe*",
                "applicationinsights:List*",
                "autoscaling-plans:Describe*",
                "aws-marketplace:Describe*",
                "aws-marketplace:List*",
                "batch:Describe*",
                "batch:List*",
                "cloudformation:Describe*",
                "cloudformation:DetectStackDrift",
                "cloudformation:DetectStackResourceDrift",
                "cloudsearch:Describe*",
                "codebuild:List*",
                "codepipeline:Get*",
                "codepipeline:List*",
                "config:DeliverConfigSnapshot",
                "config:SelectResourceConfig",
                "dax:List*",
                "deeplens:List*",
                "discovery:Describe*",
                "dlm:Get*",
                "ds:Describe*",
                "ds:List*",
                "dynamodb:Describe*",
                "dynamodb:List*",
                "ec2:Get*",
                "ecr:Describe*",
                "ecr:GetLifecyclePolicy*",
                "ecr:List*",
                "eks:Describe*",
                "eks:List*",
                "elasticache:List*",
                "elasticbeanstalk:List*",
                "elasticfilesystem:Describe*",
                "fms:List*",
                "glacier:Describe*",
                "glacier:GetDataRetrievalPolicy",
                "glacier:GetVaultLock",
                "glacier:GetVaultNotifications",
                "glue:Get*",
                "glue:List*",
                "iam:Simulate*",
                "kafka:Describe*",
                "kafka:List*",
                "kinesis:Describe*",
                "kinesis:List*",
                "kinesisanalytics:Describe*",
                "kinesisanalytics:List*",
                "kinesisvideo:Describe*",
                "kinesisvideo:List*",
                "lambda:GetAlias",
                "lambda:GetEventSourceMapping",
                "macie:List*",
                "managedblockchain:List*",
                "mq:Describe*",
                "mq:List*",
                "qldb:Describe*",
                "qldb:List*",
                "ram:Get*",
                "rds:List*",
                "redshift:List*",
                "redshift:View*",
                "resource-groups:Get*",
                "resource-groups:List*",
                "route53domains:ViewBilling",
                "servicediscovery:DiscoverInstances",
                "servicediscovery:Get*",
                "servicediscovery:List*",
                "sqs:Get*",
                "sqs:List*",
                "sso-directory:Describe*",
                "sso-directory:List*",
                "sso:Describe*",
                "states:Describe*",
                "states:Get*",
                "states:List*",
                "swf:Describe*",
                "tag:Get*",
                "wellarchitected:Get*",
                "wellarchitected:List*"
            ],
            "Resource": "*",
            "Effect": "Allow",
            "Sid": "CZResourceMonitoring20190912"
        }
    ]
}
